<% content_for :sidebar, container_menu %>

<div id="contentalt">
  <h1><%= title %></h1>

  <%= error_messages_for :photo %>

  <% form_for([ container, @photo ], :html => { :multipart => true }) do |f| -%>

    <%= render :partial => "form", :object => f %>

    <%= f.submit t(:create) %>

  <% end -%>
</div>
